<html>
    <head>
        <style>
            .header{
                height: 62px;
                overflow:hidden;
                zoom:1;
                border-bottom: 1px solid #d6dfea;
                background: #eff4fa;
            }
            div{
                display: block;
            }
            .header_link{
                overflow: hidden;
                margin-top: 16px;
                padding-right: 17px;
                line-height:30px;
                text-align: right;
                color: #b6b6b6;
                float: right;
                position: relative;
            }
            div{
                display: block;
            }

            .container{
                width: 100%;
                border-bottom: 1px solid #d6dfea;
            }

            .condition{
                width: 90%;
                height: 30px;
            }

            .condition select{
                padding: 8px 16px;
            }

            .condition input{
                padding: 8px 16px;
            }

            .frame{
                width: 100%;
                height: 830px;
                border: none;
            }
            .frame iframe{
                width: 100%;
                height: 830px;
                border: none;
            }
        </style>
    </head>
    <body>
        <div class = "header">
            <div id = "header_link" class="header_link">
                <a href="http://localhost:3000/Login" target = "_self">登录</a>
                <a href="http://localhost:3000/Login/signup">注册</a>
            </div>
        </div>
        <div class = "container">
            <form target="fra" action = "http://localhost:3000/Admin/Pages/" method = "GET">
                <div class = "condition">
                    评审领域
                    <select name = "judgeField">
                        <option value = ""></option>
                        <option value = "幼儿园评估">幼儿园评估</option>
                        <option value = "小学教育评估">小学教育评估</option>
                        <option value = "中学教育评估">中学教育评估</option>
                        <option value = "中职教育评估">中职教育评估</option>
                        <option value = "高职教育评估">高职教育评估</option>
                        <option value = "高校教育评估">高校教育评估</option>
                    </select>
                    状态
                    <select name = "status">
                        <option value = ""></option>
                        <option value = "可用">可用</option>
                        <option value = "待评估">待评估</option>
                        <option value = "填写中">填写中</option>
                        <option value = "失效">失效</option>
                        <option value = "已驳回">已驳回</option>
                    </select>
                    <input type = "submit" value = "查询"/>
                </div>
            </form>
            <div class = "frame">
                <iframe name = "fra" src = "http://localhost:3000/Admin/Pages?judgeField=&status=&currPage=1" frameborder = "0" scrolling = "no"/>
            </div>
        </div>
    </body>
</html>